{
    "name": "wmandai/repositories",
    "description": "Flexible Laravel Repository Library. Credit: SebastianBerc",
    "keywords": [
        "league",
        "repository"
    ],
    "homepage": "https://github.com/SebastianBerc/Repositories",
    "license": "MIT",
    "authors": [{
        "name": "William Mandai",
        "email": "wm@gitbench.com",
        "homepage": "https://workice.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.3",
        "illuminate/contracts": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.4",
    },
    "autoload": {
        "psr-4": {
            "SebastianBerc\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastianBerc\\Repositories\\Test\\": "tests"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}